@model IEnumerable<UnamiSushi.Models.MenuItem>

@{
    Layout = null;
}

<h2>MenuItem Index</h2>

@Html.ActionLink("Back to Master Index", "Admin", "Home")

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MenuItemID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubcategoryID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuItemName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuItemDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuItemPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Piece)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cooked)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vegetarian)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MenuItemID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubcategoryID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuItemName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuItemDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuItemPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Piece)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cooked)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vegetarian)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MenuItemID }) |
            @Html.ActionLink("Details", "Details", new { id=item.MenuItemID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MenuItemID })
        </td>
    </tr>
}

</table>
